{
	"extends": ["plugin:@typescript-eslint/recommended", "plugin:react/recommended", "plugin:react-hooks/recommended", "plugin:react/jsx-runtime", "plugin:prettier/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 2021
	},
	"plugins": ["@typescript-eslint"],
	"env": {
		"browser": true,
		"webextensions": true
	},
	"rules": {
    "react/prop-types": "off"
  },
	"settings": {
    "react": {
      "version": "detect"
    }
  }
}
